<svg xmlns="http://www.w3.org/2000/svg" width="400" height="600" viewBox="0 0 400 600">
  <style>
    .title { font: bold 24px sans-serif; }
    .input { fill: #f8f9fa; stroke: #dee2e6; }
    .button { fill: #e7f3ff; stroke: #0d6efd; }
    .button-text { fill: #0d6efd; font: 14px sans-serif; }
    .cached { fill: #f8f9fa; stroke: #dee2e6; }
    .text { font: 14px sans-serif; }
  </style>
  
  <rect width="100%" height="100%" fill="white"/>
  
  <text x="20" y="40" class="title">Caching Demo</text>
  
  <rect x="20" y="70" width="360" height="40" rx="4" class="input"/>
  <text x="30" y="95" class="text">hello world</text>
  
  <rect x="20" y="130" width="120" height="35" rx="4" class="button"/>
  <text x="35" y="153" class="button-text">Save to Cache</text>
  
  <rect x="20" y="185" width="360" height="40" rx="4" class="cached"/>
  <text x="30" y="210" class="text">Cached Value</text>
  <text x="30" y="210" class="text">hello world</text>
</svg>